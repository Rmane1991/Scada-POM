		https://mvnrepository.com/artifact/com.applitools/eyes-selenium-java5 -->
		<dependency>
			<groupId>com.applitools</groupId>
			<artifactId>eyes-selenium-java5</artifactId>
			<version>5.80.0</version>
		</dependency>

		<!-- https://mvnrepository.com/artifact/net.sf.jazzy/jazzy-core -->
		<dependency>
			<groupId>net.sf.jazzy</groupId>
			<artifactId>jazzy-core</artifactId>
			<version>0.5.2</version>
		</dependency>


		<dependency>
			<groupId>org.languagetool</groupId>
			<artifactId>language-en</artifactId>
			<version>6.3</version> <!-- Use latest if needed -->
		</dependency>


		<!-- LanguageTool Core Library -->
		<dependency>
			<groupId>org.languagetool</groupId>
			<artifactId>languagetool-core</artifactId>
			<version>6.3</version>
		</dependency>


